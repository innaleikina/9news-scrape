<div class="module">

</div>
<h2> Favorite Articles </h2>
<div id="saved-wrap">

    {{#each articles}}
    <div class="one-article saved-article">
        <div class="title-btn-wrap">
            <a target="_blank" href="{{this.link}}">
                <p> {{this.title}}</p>
            </a>
            <div class="btn-wrap-saved">
                <button class="add-note all-btn" data-id={{this._id}}> add note </button>
                <button class="delete-fave all-btn" data-id={{this._id}}> delete article </button>
            </div>
        </div>
        <div class="notes-wrap">
            {{#each this.note}}
            <div class="one-note">
                {{this.body}}
            </div>
            {{/each}}
        </div>

    </div>
    {{/each}}
</div>





</div>